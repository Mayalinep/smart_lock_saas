        {
          "expr": "sum(rate(business_email_sent_total[1m])) by (type)",
          "legendFormat": "{{type}}",
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "${datasource}" }
        }
      ]
    },
    {
      "type": "bargauge",
